---
export type Props = {
  dateModified?: Date
  datePublished?: Date
}

const { dateModified, datePublished } = Astro.props
---

{
  datePublished && (
    <meta
      property="article:published_time"
      content={datePublished.toISOString()}
    />
  )
}
{
  dateModified && (
    <meta
      property="article:modified_time"
      content={dateModified.toISOString()}
    />
  )
}
