{
  "name": "gkx-site-2024",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host --open",
    "check": "astro check",
    "build": "astro check && astro build",
    "serve": "netlify serve",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "sb:login": "storyblok logout && storyblok login",
    "sb:pull": "storyblok pull-components --space 300625 --path ./storyblok/ && storyblok-generate-ts source=./storyblok/components.300625.json target=./src/types/storyblok.d.ts",
    "sb:push": "node scripts/prompt-sb-push.js",
    "sb:pull-seperate": "storyblok pull-components --space 300625 --separate-files --file-name production --path ./storyblok/components/",
    "ssl:create": "mkcert localhost",
    "ssl": "local-ssl-proxy --source 4000 --target 4321 --cert localhost.pem --key localhost-key.pem",
    "ssl:serve": "local-ssl-proxy --source 8000 --target 8888 --cert localhost.pem --key localhost-key.pem"
  },
  "dependencies": {
    "@alpinejs/focus": "^3.14.3",
    "@alpinejs/persist": "^3.14.1",
    "@astrojs/alpinejs": "^0.4.0",
    "@astrojs/check": "^0.9.3",
    "@astrojs/netlify": "^5.5.1",
    "@astrojs/tailwind": "^5.1.0",
    "@darkroom.engineering/tempus": "^0.0.46",
    "@storyblok/astro": "^5.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "alpinejs": "^3.14.1",
    "astro": "^4.15.2",
    "astro-icon": "^1.1.2",
    "clsx": "^2.1.1",
    "gsap": "npm:@gsap/shockingly@^3.12.5",
    "lodash": "^4.17.21",
    "normalize-url": "^8.0.1",
    "player.js": "^0.1.0",
    "plyr": "^3.7.8",
    "slugify": "^1.6.6",
    "storyblok-rich-text-astro-renderer": "^2.2.0",
    "svg-sprite": "^2.0.4",
    "swiper": "^11.1.12",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3"
  },
  "devDependencies": {
    "@types/alpinejs": "^3.13.10",
    "child_process": "^1.0.2",
    "local-ssl-proxy": "^2.0.5",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "readline": "^1.3.0",
    "storyblok-generate-ts": "^2.1.0"
  }
}
